{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -p tsconfig.prod.json && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-favicon": "pwa-assets-generator --config pwa-assets.config.ts && cp src/assets/logo.svg public/logo.svg",
    "generate-pwa-icons": "npx pwa-asset-generator src/assets/logo.svg public/assets -i index.html -m public/manifest.webmanifest --padding 15% --opaque false --type png --maskable --path-override /assets --icon-only",
    "generate-pwa-splash-light": "npx pwa-asset-generator src/assets/logo.svg public/assets -i index.html -m public/manifest.webmanifest --padding 15% --opaque false --type png --maskable --path-override /assets --background white --splash-only",
    "generate-pwa-splash-dark": "npx pwa-asset-generator src/assets/logo.svg public/assets -i index.html -m public/manifest.webmanifest --padding 15% --opaque false --type png --maskable --path-override /assets --background black --dark-mode --splash-only",
    "format-index": "prettier --write index.html",
    "generate-all-pwa-assets": "yarn generate-favicon && yarn generate-pwa-icons && yarn generate-pwa-splash-light && yarn generate-pwa-splash-dark && yarn format-index"
  },
  "dependencies": {
    "@ericblade/quagga2": "^1.8.4",
    "@hookform/resolvers": "^5.2.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.125.6",
    "@tanstack/react-router-devtools": "^1.125.6",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/zod-adapter": "^1.128.4",
    "@techstark/opencv-js": "^4.11.0-release.1",
    "blurhash": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.0.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.7",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-barcode": "^1.6.1",
    "react-blurhash": "^0.3.0",
    "react-dom": "^19.1.0",
    "react-easy-crop": "^5.5.0",
    "react-fast-marquee": "^1.6.5",
    "react-hook-form": "^7.61.1",
    "react-webcam": "^7.2.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tesseract.js": "^6.0.1",
    "use-mask-input": "^3.5.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tanstack/router-plugin": "^1.125.6",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-svgr": "^4.3.0"
  }
}
